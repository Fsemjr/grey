import cv2
import numpy as np
def sketch(imagem):
    img_gray = cv2.cvtColor(image, cv2.COLLOR_BGR2GRAY)
    img_gray_blur = cv2.GaussianBlur(img_gray(5,5),0)
    canny_edges = cv2.threshold(canny_edges, 70, 255, cv2.THRESH_BINARY_INV)
    return mask
cap = cv2.VideoCapture(0)
while True:
    ret, frame = cap.read()
    cv2.imshow('Our live sketcher', sketch(frame))
    if cv2.waikey(1) == 13: #13 us the enter key
        break
cap.release()
cv2.destroyALLWindowns()        
